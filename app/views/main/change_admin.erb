<div class="main admin-change">
  <div class="container">

    <div class="col-md-4">
    	<form method="post" action="/group/change_admin/">
    		<% @user_list.each do |user| %>
    			<input type="radio" name="user_id" value="<%=user.id%>"> <%= user.user_name %> </br>
    		<% end %>
    		<button type="submit" class="btn">submit</button>
    	</form>
    </div>

    <div class="col-md-4">
      <ul>
        <h3>what can an admin do?</h3>
        <li><span class="glyphicon glyphicon-star-ok" aria-hidden="true" style="color: green;"></span>&nbsp;</li>
      </ul>
    </div>

  </div>
</div>